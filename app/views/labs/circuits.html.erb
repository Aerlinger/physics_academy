<%= provide(:title, "Circuit Simulator") %>

<!-- Toolbar containing available circuit elements -->
<div id="toolbar">
  <ul id="toolbar_buttons">
    <li class="testedworking"><a href="#" onClick="CirSim.resetSelection();">Select</a></li>
    <li class="testedworking"><a href="#" onClick="CirSim.deleteSelected();">Delete Selected</a></li>
  </ul>
</div>

<div id="workspace">
  <!-- Circuit rendering canvas -->
  <canvas id="canvas_container" width="800" height="600" class="opint"></canvas>
  <div id="edit_pane"></div>
</div>

<!-- Column of sample circuits -->
<div id="circuit_presets">
</div>